/dts-v1/;

#include "imx6dl.dtsi"
#include "imx6qdl-advantech.dtsi"

/ {
	model = "Freescale i.MX6 Solo ROM7421 A1";
	compatible = "fsl,imx6dl-sabresd", "fsl,imx6dl";

	aliases {
		i2c3; /* Disabled i2c3 in imx6dl.dtsi */
	};

	board {
		compatible = "proc-board";
		board-type = "ROM-7421 A1";
		board-cpu = "DualLiteSolo";
	};

	regulators {
		reg_usb_otg_vbus: usb_otg_vbus {
			compatible = "regulator-fixed";
			regulator-name = "usb_otg_vbus";
			regulator-min-microvolt = <5000000>;
			regulator-max-microvolt = <5000000>;
			gpio = <&gpio3 23 GPIO_ACTIVE_HIGH>;
			enable-active-high;
		};
	};

	gpio-keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-0 = <&pinctrl_gpio_power_1 &pinctrl_gpio_sleep_1>;
		pinctrl-names = "default";
		power@1 {
			label = "Power button";
			gpios = <&gpio3 24 1>; /* GPIO_ACTIVE_LOW:1 */
			linux,input-type = <22>;	/* EV_PWR */
			linux,code = <116>;		/* KEY_POWER */
			debounce_interval = <1>;
                };
		sleep@2 {
			label = "Sleep Button";
			gpios = <&gpio4 5 1>;
			linux,input-type = <1>;		/* EV_KEY */
			linux,code = <205>;		/* KEY_SUSPEND */
			debounce_interval = <1>;
			gpio-key,wakeup;
		};
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_leds_status_1>;
		status-led {
			label = "Heartbeat";
			gpios = <&gpio1 4 1>;
			linux,default-trigger = "heartbeat";
		};
	};

	vbat: fixedregulator@0 {
		compatible = "regulator-fixed";
		regulator-name = "vbat";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-boot-on;
	};

	camera-leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_leds_status_2>;
		status-led {
			label = "Camera_Status";
			gpios = <&gpio6 5 1>;
			linux,default-trigger = "heartbeat";
		};
	};
};

&audio_sgtl5000 {
	audio-codec = <&codec>;
	status = "okay";
};

&bkl {
	lvds-bkl-enable = <&gpio6 9 1>;
	lvds-vcc-enable = <&gpio6 7 1>;
	pwms = <&pwm1 0 5000000>;
	status = "okay";
};

&ecspi1 {
	cs-gpios = <&gpio3 19 0>;
};

