&i2c4 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c4>;
	status = "okay";

	24c32@50 {
		compatible = "fsl,24c64";
		pagesize = <32>;
		reg = <0x50>;
	};
};

&gpmi {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_gpmi_nand_1>;
	status = "disabled";
	nand-on-flash-bbt;
};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog_1 &pinctrl_hog_3>;

	imx7d-sdb {
		pinctrl_hog_1: hoggrp-1 {
			fsl,pins = <
				/* MX7D_PAD_ECSPI2_SS0__GPIO4_IO23	  0x19 */ /* BT_REG_ON */
				MX7D_PAD_ENET1_RX_CLK__GPIO7_IO13 0x59 /* WDT Enable */
				MX7D_PAD_ENET1_CRS__GPIO7_IO14 	  0x59 /* WDT Reset */
			>;
		};

		pinctrl_hog_3: hoggrp-3 { //alex
			fsl,pins = <
				MX7D_PAD_GPIO1_IO12__GPIO1_IO12		0x59	/* GPIO0 - GPIO0 */
				MX7D_PAD_GPIO1_IO13__GPIO1_IO13		0x59	/* GPIO1 - GPIO1 */
				MX7D_PAD_GPIO1_IO14__GPIO1_IO14		0x59	/* GPIO2 - GPIO2 */
				MX7D_PAD_GPIO1_IO15__GPIO1_IO15		0x59	/* GPIO3 - GPIO3 */
				/* GPIO4 - BOOT_CFG04 */
				/* GPIO5 - BOOT_CFG05 */
				/* GPIO6 - BOOT_CFG06 */
				MX7D_PAD_ECSPI2_SCLK__GPIO4_IO20	0x59	/* GPIO7  - GPIO7 */
				MX7D_PAD_ECSPI2_MISO__GPIO4_IO22	0x59	/* GPIO8  - GPIO8 */
				MX7D_PAD_ECSPI2_MOSI__GPIO4_IO21	0x59	/* GPIO9  - GPIO9 */
				MX7D_PAD_ECSPI2_SS0__GPIO4_IO23		0x59	/* GPIO10 - GPIO10 */
				/* GPIO11 - BOOT_CFG11 */
			>;
		};

/*
		pinctrl_epdc_elan_touch: epdc_elan_touch_grp {
			fsl,pins = <
				MX7D_PAD_SAI1_RX_DATA__GPIO6_IO12 0x59
				MX7D_PAD_SAI1_TX_DATA__GPIO6_IO15 0x1b
				MX7D_PAD_SAI1_TX_BCLK__GPIO6_IO13 0x80000000
