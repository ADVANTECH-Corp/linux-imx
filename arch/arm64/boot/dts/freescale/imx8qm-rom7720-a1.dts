#	undef  LVDS_SINGLE_1
#elif DISPLAY_MODE == HDMI_ONLY
#	define  HDMI_DISPLAY
#	undef LVDS_DISPLAY
#elif DISPLAY_MODE == HDMI_AND_LVDS_CH0
#	define HDMI_DISPLAY
#	define LVDS_DISPLAY
#	define LVDS_SINGLE_0
#	undef  LVDS_SINGLE_1
#elif DISPLAY_MODE == HDMI_AND_LVDS_CH1
