                pinctrl_sleep_key: gpio_userkeygrp-2 {
                        fsl,pins = <
				MX8MM_IOMUXC_GPIO1_IO01_GPIO1_IO1		0x19	/* SLEEP */
                        >;
                };

                pinctrl_wdt_en_1: wdt_engrp-1 {
                        fsl,pins = <
                                MX8MM_IOMUXC_GPIO1_IO09_GPIO1_IO9		0x19    /* WDT_EN */
                        >;
